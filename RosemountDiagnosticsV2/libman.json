{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.5.1",
      "destination": "wwwroot/lib/jquery/jquery/"
    },
    {
      "library": "twitter-bootstrap@4.4.0",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "library": "datatable@2.0.1",
      "destination": "wwwroot/lib/datatable/"
    },
    {
      "provider": "unpkg",
      "library": "@aspnet/signalr@1.1.4",
      "destination": "wwwroot/lib/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.js.map",
        "dist/browser/signalr.min.js",
        "dist/browser/signalr.min.js.map"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "bootstrap@4.4.0",
      "destination": "wwwroot/bootstrap/"
    }
  ]
}